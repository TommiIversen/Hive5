{
  "$schema": "https://json.schemastore.org/launchsettings.json",
    "profiles": {
      "streamhub_http_9000": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "hotReloadEnabled": true,
        "applicationUrl": "http://localhost:9000",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "streamhub_http_8999": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "hotReloadEnabled": true,
        "applicationUrl": "http://localhost:8999",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "https": {
        "commandName": "Project",
        "dotnetRunMessages": true,
        "launchBrowser": true,
        "applicationUrl": "https://localhost:7074;http://localhost:5087",
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "Watch 8999": {
        "commandName": "Executable",
        "workingDirectory": "$(ProjectDir)",
        "executablePath": "dotnet",
        "commandLineArgs": "watch --project StreamHub.csproj  run debug --launch-profile streamhub_http_8999"
      },
      "Watch 9000": {
        "commandName": "Executable",
        "workingDirectory": "$(ProjectDir)",
        "executablePath": "dotnet",
        "commandLineArgs": "watch --project StreamHub.csproj run debug --launch-profile streamhub_http_9000"      }
    }
  }
